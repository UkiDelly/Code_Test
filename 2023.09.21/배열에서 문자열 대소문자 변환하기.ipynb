   "source": [
    "# 배열에서 문자열 대소문자 변환하기\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['aaa', 'BBB', 'ccc', 'DDD']\n"
     ]
    }
   ],
   "source": [
    "def solution(str_arr: list[str]):\n",
    "    for index, s in enumerate(str_arr):\n",
    "        if index % 2 != 0:\n",
    "            str_arr[index] = s.upper()\n",
    "        else:\n",
    "            str_arr[index] = s.lower()\n",
    "    return str_arr\n",
    "\n",
    "\n",
    "print(solution([\"AAA\", \"BBB\", \"CCC\", \"DDD\"]))"
   ]
  }
 ],
